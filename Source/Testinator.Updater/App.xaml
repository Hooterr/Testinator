<Application x:Class="Testinator.Updater.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Testinator.Updater">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                
                <!-- Order is important -->
                <ResourceDictionary Source="pack://application:,,,/Testinator.UICore;component/Styles/Colors.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Testinator.UICore;component/Styles/Fonts.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Testinator.UICore;component/Styles/Texts.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Testinator.UICore;component/Styles/ProgressBars.xaml" />
                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
